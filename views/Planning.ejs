<body>
 
  <div class="tableau-membres">
    
    <ul>
      <li>instructeur 6 : mourad louhichi  </li>
     <li>instructeur 7 : macaire  alex </li> 
     
    </ul>
  
  <table>
    <thead>
    <tr>
      <th>ID</th>
      <th>JOUR</th>
      <th>HEURE DEBUT</th>
      <th>HEURE FIN</th>
      <th>Nom de l'instructeur</th>
     <th>Prénom de l'instructeur</th>
      <th>TYPE COURS</th>
      <th>Actions</th>
    </tr>
  </thead>
    <% for(let i=0; i < planning.length; i++) { %>
      
        <tr>
          <td><%= planning[i].ID %></td>
          <td><%= planning[i].JOUR %></td>
          <td><%= planning[i].HEURE_DEBUT %></td>
          <td><%= planning[i].HEURE_FIN %></td>
          <td><%= planning[i].Instructeur_Nom %></td>
          <td><%= planning[i].Instructeur_Prénom %></td>
          <td><%= planning[i].TYPE_COURS %></td>
         
         <td><button><a href="/updatePlanning/<%= planning[i].ID %>">Modifier</a></button> </td> 
         
          <td><button><a  href="/deleteCompletePlanning/<%= planning[i].ID %>">Supprimer la ligne </a></button></td>
      
      </tr>
    <% }; %>
  </table>
  <div class="planning-container">
   

  
  
   
